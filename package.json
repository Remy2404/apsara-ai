  "description": "Apsara AI is an advanced intelligent AI-powered application platform designed to deliver cutting-edge artificial intelligence solutions.",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "start:frontend": "lerna run start --scope=@apsara-ai/frontend",
