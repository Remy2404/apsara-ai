    "test": "lerna run test",
    "clean": "lerna clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Remy2404/apsara-ai.git"
  },
  "keywords": [
    "ai",
    "chatbot",
    "expo",
    "react-native",
    "express",
    "nodejs"
  ],
